    {
      "id": "ai_chat_intro",
      "type": "ai_chat",
      "name": "AI Chat Introduction",
      "message": "ü§ñ AI Chat Assistant\n\nYou're about to enter our AI-powered chat assistant. This intelligent assistant can help you with:\n\n‚úì General questions about our services\n‚úì Product information and recommendations\n‚úì Troubleshooting common issues\n‚úì Account inquiries\n‚úì And much more!\n\nThe AI assistant has access to your account context and chat history to provide personalized assistance.\n\nType 'exit' or 'menu' at any time to return to the main menu.\n\nReady to start? Type anything to begin chatting with our AI assistant!",
      "collect_input": true,
      "input_type": "text",
      "ai_config": {
        "llm_model": "qwen2.5:7b-instruct-q5_K_S",
        "system_prompt": "You are a helpful customer support AI assistant. You have access to the customer's account information and chat history. Be friendly, professional, and concise. If you cannot help with something, suggest transferring to a human agent. Always maintain context from previous messages in the conversation.",
        "max_history_messages": 10,
        "include_chat_history": true,
        "context_variables": [
          "user_email",
          "user_id",
          "account_type",
          "previous_issues"
        ],
        "exit_keywords": [
          "exit",
          "menu",
          "main menu",
          "quit",
          "stop"
        ],
        "exit_node_id": "ai_chat_exit"
      },
      "transitions": [
        {
          "condition": null,
          "target_node_id": "ai_chat_node",
          "label": "Start AI Chat"
        }
      ]
    },
