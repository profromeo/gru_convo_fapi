{
    "id": "node_analyze_image",
    "type": "process_media",
    "name": "Analyze Damage Image",
    "process_media_config": {
        "action_type": "ai_service",
        "output_variable": "ai_analysis_result",
        "ai_service_config": {
            "query": "Assess the damage shown in this image. Is it severe?",
            "system_message": "You are an expert insurance claim adjuster.",
            "llm_model": "llava-v1.6",
            "llm_provider": "ollama",
            "temperature": 0.2,
            "max_tokens": 500,
            "include_chat_history": true,
            "max_history_messages": 5,
            "metadata": {
                "claim_id": "{{context.claim_id}}"
            }
        }
    },
    "default_transition": "node_show_assessment"
}